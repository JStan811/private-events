<h1><%= @user.username %></h1>

<p>Email: <%= @user.email %></p>

<p>Created Events:</p>
<ul>
  <% @created_events.each do |event| %>
    <li>
      <%= event.title %> | Date: <%= event.date %> | Location: <%= event.location %>
    </li>
  <% end %>
</ul>

<p>Attended Events:</p>
<ul>
  <% @user.attended_events.each do |event| %>
    <li>
      <%= event.title %> | Date: <%= event.date %> | Location: <%= event.location %>
    </li>
  <% end %>
</ul>


<%= button_to "back to users page", users_path, method: :get  %>
